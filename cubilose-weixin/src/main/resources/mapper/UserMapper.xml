<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cubilose.weixin.mapper.UserMapper">

	<sql id="BASE_COLUMN">
		w_id, register_time
	</sql>

	<resultMap id="listMap" type="User">
		<result column="id" property="id" />
		<result column="w_id" property="wId" />
		<result column="register_time" property="registerTime" />
	</resultMap>

	<select id="list" resultType="java.util.List" resultMap="listMap">
		SELECT id, <include refid="BASE_COLUMN" />
		FROM user
	</select>

</mapper>